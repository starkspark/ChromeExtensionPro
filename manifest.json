{
    "name": "Earth's Asteroids",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Keeping up with asteroids headed Earth's way.",
    "background": {
        "persistent": false,
        "scripts": ["javascript.js"]
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "index.html" 
    }
}